#include <windows.h>
#include <vector>
#include <iostream>
#include <string>
#include <vector>
#include <array>






int main() {

	std::vector <int> number = { 1 , 2 , 3 , 4 , 5};
	std::vector <bool> boll = { true , true , false , false };
	std::vector <std::string>  list = { "fedyka " , "pioras" , "tomov" };
	std::cout << number[2 ] << std::endl; std::cout << boll[2] << std::endl; std::cout << list[2] << std::endl;
	std::cout << number.size() << std::endl;
	number.push_back(3);
	number.emplace_back();
	if (number.empty()) { // пустой массив 
		
	}
	for (auto test : number) {
		std::cout << test << std::endl;
	};
	number.~vector();

	
	std::array <int, 2 > numbers1 = { 1  , 2};
	for (auto test1 : numbers1) {
		std::cout << test1;
	}
	return 0;



       // 	int a , b , c , d , e ;
	std::cin >> a;
	std::cin >> b;
	std::cin >> c;
	std::cin >> d;
	std::cin >> e;
	std::vector <int> number = { a , b , c  , d  , e };
	for (auto test : number) {
		std::cout << test;
	}



	
}
